{
  "breadcrumbs.enabled": true,
  "editor.fontFamily": "MonoLisa",
  "editor.minimap.enabled": false,
  "editor.cursorSmoothCaretAnimation": true,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.fontSize": 14,
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "editor.smoothScrolling": true,
  "editor.accessibilitySupport": "off",
  "editor.scrollBeyondLastLine": true,
  "editor.fontLigatures": true,
  "editor.wordWrap": "on",
  "editor.unicodeHighlight.invisibleCharacters": false,
  "explorer.confirmDelete": false,
  "explorer.compactFolders": false,
  "explorer.confirmDragAndDrop": false,
  "workbench.startupEditor": "none",
  "workbench.preferredLightColorTheme": "mlia light",
  "workbench.preferredDarkColorTheme": "mlia dark",
  "workbench.colorTheme": "mlia dark",
  "workbench.fontAliasing": "antialiased",
  "workbench.activityBar.visible": false,
  "vscode-neovim.useCtrlKeysForNormalMode": true,
  "window.autoDetectColorScheme": true,
  "vscode-neovim.neovimInitVimPaths.darwin": "~\/.config\/nvim\/init.vim",
  "vscode-neovim.useCtrlKeysForInsertMode": true,
  "vscode-neovim.neovimExecutablePaths.darwin": "\/opt\/homebrew\/bin\/nvim",
  "[go]": {
    "editor.tabSize": 4
  },
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  }
}